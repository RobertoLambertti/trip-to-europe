{
  "name": "trip-to-europe",
  "version": "1.0.0",
  "description": "В этом туре вы откроете для себя Европу в новом свете! Вас ждет: горы, море, а также потрясающая красота природы!",
  "main": "./build/index.html",
  "scripts": {
    "editorconfig": "editorconfig-cli",
    "stylelint": "stylelint \"source/sass/**/*.scss\"",
    "test": "npm run editorconfig && npm run stylelint",
    "build": "gulp build",
    "start": "gulp start"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/RobertoLambertti/trip-to-europe.git"
  },
  "author": "NikitaSech",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/RobertoLambertti/trip-to-europe/issues"
  },
  "homepage": "https://github.com/RobertoLambertti/trip-to-europe#readme",
  "engines": {
    "node": "v16.14.2"
  },
  "browserslist": [
    "last 2 versions",
    "not dead",
    "not ie <= 11"
  ],
  "editorconfig-cli": [
    "*.json",
    "*.js",
    "source/*.html",
    "source/js/**/*.js",
    "source/img/**/*.svg",
    "source/sass/**/*.scss"
  ],
  "devDependencies": {
    "@htmlacademy/editorconfig-cli": "1.0.0",
    "autoprefixer": "10.4.4",
    "browser-sync": "2.27.9",
    "del": "6.0.0",
    "gulp": "4.0.2",
    "gulp-concat": "2.6.1",
    "gulp-csscomb": "3.1.0",
    "gulp-csso": "4.0.1",
    "gulp-htmlmin": "5.0.1",
    "gulp-imagemin": "7.1.0",
    "gulp-plumber": "1.2.1",
    "gulp-postcss": "9.0.1",
    "gulp-posthtml": "3.0.5",
    "gulp-rename": "2.0.0",
    "gulp-sass": "5.1.0",
    "gulp-sourcemaps": "3.0.0",
    "gulp-svgstore": "9.0.0",
    "gulp-uglify-es": "3.0.0",
    "gulp-webp": "4.0.1",
    "posthtml-include": "1.4.3",
    "sass": "1.50.0",
    "stylelint": "14.6.1",
    "stylelint-config-standard-scss": "3.0.0"
  }
}
